{
  "name": "oakland-services",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://oakland-services.onrender.com/",
  "scripts": {
    "postinstall": "npm run db:migrate",
    "start": "node server/index.js",
    "dev": "concurrently \"nodemon server/index.js\" \"react-scripts start\"",
    "build": "npx react-scripts build",
    "seed:format": "cd db/seeds/data && node formatter.js",
    "db:migrate": "npx knex migrate:latest",
    "db:seed": "npx knex seed:run",
    "format": "prettier --write \"{src,server}/**/*.js\""
  },
  "dependencies": {
    "@react-google-maps/api": "^1.13.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "helmet": "^4.2.0",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "i18next-http-middleware": "^3.0.6",
    "json-formatter-js": "^2.3.4",
    "knex": "^0.21.12",
    "lodash": "^4.17.20",
    "pg": "^8.5.1",
    "query-string": "^6.13.7",
    "raw.macro": "^0.4.2",
    "react": "^17.0.1",
    "react-content-loader": "^5.1.4",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.3",
    "react-modal": "^3.12.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "csv-parser": "^2.3.3",
    "he": "^1.2.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-scripts": "^4.0.0",
    "sanitize-html": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "server/*"
    ]
  },
  "proxy": "http://localhost:5000"
}
